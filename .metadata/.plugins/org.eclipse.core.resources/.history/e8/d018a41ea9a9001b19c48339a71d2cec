#ifndef COMPRA_H_
#define COMPRA_H_



typedef struct
{
	int idCliente;
	int cantidadDeBarbijos;
	char direccionDeEntrega[64];
	char color[32];
	int idVenta;
	int importePagado;
	int estadoDelCobro;
	int isEmpty;
}Compra;

int compra_inicializarCompras(Cliente* listadoCompras, int len);


#endif /* COMPRA_H_ */
