#ifndef CLIENTE_H_
#define CLIENTE_H_

typedef struct
{
	char nombre[32];
	char apellido[32];
	char cuit[12];
	int idCliente;
	int isEmpty;
}Cliente;

int cliente_inicializarClientes(Cliente* listadoClientes, int len);
int cliente_buscarIndexEmtpy(Cliente* listadoClientes, int len);
int cliente_altaCliente(Cliente* listadoClientes, int len, int idCliente);



#endif /* CLIENTE_H_ */
